    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run in development",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--flavor",
                "development",
                "--target",
                "lib/application/bin/environments/main_development.dart"
            ]
        },
        {
            "name": "Run in staging",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "args": [
                "--flavor",
                "staging",
                "--target",
                "lib/application/bin/environments/main_staging.dart"
            ]
        },
        {
            "name": "Run in production",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "args": [
                "--flavor",
                "production",
                "--target",
                "lib/application/bin/environments/main_production.dart"
            ]
        }
    ],
}